{
  "name": "express-typescript-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for RESTful API using Typescript + Express + Mongoose",
  "homepage": "https://github.com/duyk16/express-typescript-boilerplate#readme",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon",
    "test": "NODE_ENV=test mocha -r ts-node/register **/*.spec.ts --recursive --timeout 5000 --watch-extensions ts --watch",
    "test:jest": "jest --watch --runInBand",
    "build": "rimraf ./build/test && tsc",
    "start:product": "npm run build && NODE_ENV=production node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duyk16/express-typescript-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/duyk16/express-typescript-boilerplate/issues"
  },
  "dependencies": {
    "@typegoose/typegoose": "^6.2.1",
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.1",
    "@types/mongoose": "^5.5.41",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.8",
    "@types/supertest": "^2.0.8",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
